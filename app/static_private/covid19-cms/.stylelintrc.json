{
    "processors": ["stylelint-processor-styled-components"],
    "extends": [
        "stylelint-config-standard",
        "stylelint-config-idiomatic-order",
        "stylelint-config-styled-components"
    ],
    "plugins": [
        "stylelint-scss"
    ],
    "rules": {
        "indentation": 4,
        "property-no-vendor-prefix": true,
        "number-leading-zero": null
    }
}
